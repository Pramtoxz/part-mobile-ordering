                                        android:layout_width="@dimen/margin_55"
                                        android:layout_height="@dimen/margin_55"
                                        android:layout_gravity="center"
                                        app:cardCornerRadius="@dimen/margin_14">

                                        <ImageView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:src="@drawable/ic_stock_md"/>

                                    </androidx.cardview.widget.CardView>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/margin_8"
                                        android:gravity="center"
                                        android:text="@string/lbl_order_suggestion"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/font_12"
                                        app:fontFamily="@font/opensans_semibold" />

                                </LinearLayout>


                            </FrameLayout>

                            <FrameLayout
                                android:id="@+id/vMenuCampaignPromo"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="24dp"
                                android:layout_marginRight="24dp"
                                android:layout_weight="1">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:orientation="vertical">

                                    <androidx.cardview.widget.CardView
                                        android:id="@+id/btnCampaignPromo"
                                        android:foreground="?selectableItemBackground"
                                        android:layout_width="55dp"
                                        android:layout_height="55dp"
                                        android:layout_gravity="center"
                                        app:cardCornerRadius="14dp">


                                        <ImageView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:src="@drawable/ic_promotion" />

                                    </androidx.cardview.widget.CardView>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:gravity="center"
                                        android:text="@string/lbl_promo_campaign"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/font_12"
                                        app:fontFamily="@font/opensans_semibold" />

                                </LinearLayout>


                            </FrameLayout>

